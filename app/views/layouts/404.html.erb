<!DOCTYPE html>
<html>
  <head>
    <title>White Elephant</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="background-red">
    <div class="header">
      <div class="logo-box">
        <%= image_tag("CL-Logo.png", :alt => "cooler", :class => "logo") %>
      </div>
      <div class="float-right">
        <%= link_to dashboard_index_url, class: 'btn quit-btn' do %>
          <i class="" aria-hidden="true">Back</i>
        <% end %>
      </div>
      <p class="404">404 Error</p>
      <div class="row">
          <div class="col-md-12">
            <div class="dashboard-white-elephant-logo-box">
              <div class="dashboard-white-elephant-logo">
                <%= image_tag("White-Elephant-Logo-dashboard.png", :alt => "cooler", :class => "white-elephant-logo") %> 
              </div>
            </div>
          </div>
        </div>
    </div>
    <%= yield %>
  </body>
</html>